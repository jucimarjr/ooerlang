-abstract(animal).
-constructor([constructor/2]).
-extend([falar/0, getNome/0, getIdade/0, setNome/1, setIdade/1]).

class_attributes.

Nome;
Idade.

class_methods.

constructor(Nome, Idade) ->
	self::Nome = Nome,
	self::Idade = Idade.

falar().

getNome() ->
	self::Nome.

getIdade() ->
	self::Idade.

setNome(Nome) ->
	self::Nome = Nome.

setIdade(Idade) ->
	self::Idade = Idade.
